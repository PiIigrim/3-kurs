@prefix : <http://www.example.org/transport#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

### Классы ###

:TransportationUnit rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Транспортное средство" .

:Car rdf:type owl:Class ;
    rdfs:subClassOf :TransportationUnit ;
    rdfs:label "Легковой автомобиль" .

:Truck rdf:type owl:Class ;
    rdfs:subClassOf :TransportationUnit ;
    rdfs:label "Грузовик" .

:Bus rdf:type owl:Class ;
    rdfs:subClassOf :TransportationUnit ;
    rdfs:label "Автобус" .

:Motorcycle rdf:type owl:Class ;
    rdfs:subClassOf :TransportationUnit ;
    rdfs:label "Мотоцикл" .

:Bicycle rdf:type owl:Class ;
    rdfs:subClassOf :TransportationUnit ;
    rdfs:label "Велосипед" .

### Свойства объектов ###

:hasName rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:string ;
    rdfs:label "Название" .

:hasSize rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:string ;
    rdfs:label "Размер" .

:hasEngineType rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:string ;
    rdfs:label "Тип двигателя" .

:hasEngineVolume rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:integer ;
    rdfs:label "Объем двигателя" .

:hasEnginePower rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:integer ;
    rdfs:label "Мощность двигателя" .

:hasTorque rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:double ;
    rdfs:label "Крутящий момент" .

:hasPassengerCapacity rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:integer ;
    rdfs:label "Вместимость пассажиров" .

:hasCargoCapacity rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:string ;
    rdfs:label "Грузоподъемность" .

:hasTransmissionType rdf:type owl:ObjectProperty ;
    rdfs:domain :TransportationUnit ;
    rdfs:range xsd:string ;
    rdfs:label "Тип коробки передач" .

### Экземпляры ###

:Тесла_Роадстер rdf:type owl:NamedIndividual ,
                       :Car ;
               :hasName "Тесла Роадстер" ;
               :hasPassengerCapacity 4 ;
               :hasCargoCapacity "До 1.5 метров и 200 килограмм" ;
               :hasTransmissionType "Автоматическая" .

:Грузовик_Volvo rdf:type owl:NamedIndividual ,
                     :Truck ;
             :hasName "Грузовик Volvo" ;
             :hasPassengerCapacity 3 ;
             :hasCargoCapacity "До 10 кубических метров" ;
             :hasTransmissionType "Механическая" .

:Автобус_Мерседес rdf:type owl:NamedIndividual ,
                      :Bus ;
              :hasName "Автобус Мерседес" ;
              :hasPassengerCapacity 30 ;
              :hasCargoCapacity "До 5 кубических метров" ;
              :hasTransmissionType "Автоматическая" .

:Мотоцикл_Харлей_Дэвидсон rdf:type owl:NamedIndividual ,
                                  :Motorcycle ;
                          :hasName "Мотоцикл Харлей-Дэвидсон" ;
                          :hasPassengerCapacity 2 ;
                          :hasCargoCapacity "Ограниченное пространство для маленьких предметов" ;
                          :hasTransmissionType "Механическая" .

:Велосипед_Giant rdf:type owl:NamedIndividual ,
                      :Bicycle ;
              :hasName "Велосипед Giant" ;
              :hasPassengerCapacity 1 ;
              :hasCargoCapacity "До 50 килограмм" ;
              :hasTransmissionType "Механическая" .
